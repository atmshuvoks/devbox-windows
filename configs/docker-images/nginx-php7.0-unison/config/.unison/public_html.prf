# Roots of the synchronization
root = /home/www-data
root = /var/www

# Paths to synchronize
path = public_html

# Params
fastercheckUNSAFE = true
ignorelocks = true
numericids = false
log = true
batch = true
fastcheck = true
confirmbigdel = false
#ignore = Path */.git
ignore = Path */.idea
ignore = Path */update/vendor/composer/composer/tests/Composer/Test/Autoload/Fixtures/classmap/LargeClass.php
prefer = /var/www